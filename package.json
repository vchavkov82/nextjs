{
  "name": "supabase",
  "description": "The Postgres Development Platform.",
  "version": "0.0.0",
  "author": "BA, Inc.",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "find node_modules/.pnpm -path '*/thread-stream@*/node_modules/thread-stream/test' -type d -exec rm -rf {} + 2>/dev/null || true",
    "build": "pnpm turbo run build --concurrency=96 --cache-dir=.turbo",
    "build:cms": "pnpm turbo run build --filter=cms --parallel",
    "build:design-system": "pnpm turbo run build --filter=design-system",
    "build:docs": "pnpm turbo run build --filter=docs",
    "clean": "pnpm turbo run clean --parallel && rimraf -G node_modules/{*,.bin,.modules.yaml}",
    "dev": "pnpm turbo run dev --parallel",
    "dev:start": "bash scripts/dev-kill.sh && bash scripts/start-postgres-noninteractive.sh && bash scripts/wait-for-postgres-ready.sh && pnpm dev",
    "pnpm:dev": "bash scripts/dev-kill.sh && bash scripts/start-postgres-noninteractive.sh && bash scripts/wait-for-postgres-ready.sh && pnpm turbo run dev --parallel",
    "dev:bun": "bun run turbo run dev --parallel",
    "dev:hybrid": "bun run turbo run dev --parallel",
    "kill": "bash scripts/dev-kill.sh",
    "dev:cms": "pnpm turbo run dev --filter=cms --parallel",
    "dev:docs": "pnpm turbo run dev --filter=docs --parallel",
    "dev:docs:fast": "pnpm --filter=docs exec next dev --port 3001",
    "dev:docs:bun": "cd ./apps/docs && bun run dev",
    "dev:www": "pnpm turbo run dev --filter=www --parallel",
    "dev:design-system": "pnpm turbo run dev --filter=design-system --parallel",
    "lint": "pnpm turbo run lint",
    "typecheck": "pnpm turbo --continue typecheck",
    "test:prettier": "prettier --cache --check '{apps,packages}/**/*.{js,jsx,ts,tsx,css,md,mdx,json}'",
    "format": "prettier --cache --write '{apps,packages}/**/*.{js,jsx,ts,tsx,css,md,mdx,json}'",
    "test:docs": "turbo run test --filter=docs",
    "test:ui": "turbo run test --filter=ui",
    "test:ui-patterns": "turbo run test --filter=ui-patterns",
    "e2e:setup:cli": "supabase stop --all --no-backup ; supabase start && supabase db reset && supabase status --output json > keys.json",
    "perf:kong": "ab -t 5 -c 20 -T application/json http://localhost:8000/",
    "perf:meta": "ab -t 5 -c 20 -T application/json http://localhost:5555/tables",
    "setup:cli": "supabase start && supabase status --output json > keys.json",
    "generate:types": "supabase gen types typescript --local > ./supabase/functions/common/database-types.ts",
    "api:codegen": "cd packages/api-types && pnpm run codegen",
    "knip": "pnpx knip@~5.50.0"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "eslint": "^9.0.0",
    "prettier": "3.2.5",
    "prettier-plugin-sql-cst": "^0.11.0",
    "rimraf": "^6.0.0",
    "sass": "^1.72.0",
    "supabase": "^2.65.6",
    "supports-color": "^8.0.0",
    "tailwindcss": "catalog:",
    "tsx": "catalog:",
    "turbo": "2.7.3",
    "typescript": "catalog:",
    "zod": "catalog:"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vchavkov82/nextjs.git"
  },
  "engines": {
    "pnpm": "10.24",
    "node": ">=22"
  },
  "keywords": [
    "postgres",
    "firebase",
    "storage",
    "functions",
    "database",
    "auth"
  ],
  "packageManager": "pnpm@10.24.0",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*",
      "blocks/*",
      "e2e/*"
    ],
    "catalog": {
      "@sentry/nextjs": "^10.26.0",
      "@supabase/auth-js": "2.87.0",
      "@supabase/postgrest-js": "2.87.0",
      "@supabase/realtime-js": "2.87.0",
      "@supabase/supabase-js": "2.87.0",
      "@types/node": "^22.0.0",
      "@types/react": "^18.3.0",
      "@types/react-dom": "^18.3.0",
      "next": "^16.0.10",
      "react": "^18.3.0",
      "react-dom": "^18.3.0",
      "recharts": "^2.15.4",
      "tailwindcss": "3.4.1",
      "tsx": "4.20.3",
      "typescript": "~5.9.0",
      "valtio": "^1.12.0",
      "vite": "^7.1.11",
      "vitest": "^3.2.0",
      "zod": "^3.25.76"
    }
  },
  "overrides": {
    "@eslint/eslintrc>js-yaml": "^4.1.1",
    "@opentelemetry/api": "<1.9.0",
    "@react-router/dev>vite-node": "3.2.4",
    "@redocly/respect-core>form-data": "^4.0.4",
    "@redocly/respect-core>js-yaml": "^4.1.1",
    "@tanstack/directive-functions-plugin>vite": "catalog:",
    "@tanstack/react-start-plugin>vite": "catalog:",
    "esbuild": "^0.25.2",
    "nodemailer": "^7.0.11",
    "refractor>prismjs": "^1.30.0",
    "tar": "^7.0.0",
    "tmp": "^0.2.4",
    "vinxi>vite": "catalog:",
    "prettier": "3.2.5",
    "vfile-matter": "^3.0.1"
  },
  "dependencies": {
    "@supabase/sql-to-rest": "^0.1.6"
  }
}
